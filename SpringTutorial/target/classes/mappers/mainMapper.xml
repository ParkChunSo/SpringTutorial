<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.dev.mapper.mainMapper">

	<select id="getCosmos" parameterType="int" resultType="CosmosVO">
		SELECT no, name
		FROM info_task
		WHERE no = #{no}
	</select>
	
</mapper>